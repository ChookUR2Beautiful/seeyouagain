package com.xmn.designer.controller.api.v1.material.vo;

import javax.validation.constraints.NotNull;

import com.xmn.designer.base.Request;

public class MaterialCategoryRequest extends Request{
    
    /**
     * 
     */
    private static final long serialVersionUID = 7663407272817611209L;



    @NotNull(message = "搜索类型不能为空")
    private String type;
    
    
    
    private Long id;


    public String getType() {
        return type;
    }


    public void setType(String type) {
        this.type = type;
    }

    public Long getId() {
        return id;
    }


    public void setId(Long id) {
        this.id = id;
    }


    @Override
    public String toString() {
        return "MaterialListRequest [type=" + type + ", id=" + id + "]";
    }
    
    
    

}
