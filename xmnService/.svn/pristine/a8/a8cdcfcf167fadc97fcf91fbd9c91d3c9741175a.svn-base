package com.xmniao.xmn.core.catehome.response;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 
* @projectName: xmnService 
* @ClassName: SpecilTopicSubInfoResponse    
* @Description:专题关联的其他信息   
* @author: liuzhihao   
* @date: 2017年2月17日 下午3:44:36
 */
@SuppressWarnings("serial")
public class SpecilTopicSubInfoResponse implements Serializable{

	private Integer subId;//关联ID
	
	private String breviaryImage;//缩略图 如 套餐图片、店铺图片、预告封面、预售封面、主播图像
	
	private String sellerName;//商铺名称
	
	private String sellerTradeName;//商铺分类名称
	
	private Integer consumes;//消费人次
	
	private String zoneName;//商圈名称
	
	private String comboName;//套餐名称
	
	private BigDecimal comboPrice;//套餐价格
	
	private BigDecimal comboBird;//套餐鸟币
	
	private String preTitle;//预售标题、预告标题
	
	private String liverName;//主播名称
	
	private Integer sex;//性别
	
	private Integer isFollow;//是否关注标签
	
	private Integer roomNo;//直播房间号
	
	private String startDate;//预告时间
	
	private BigDecimal fansPrice;//粉丝卷面额

	public Integer getSubId() {
		return subId;
	}

	public void setSubId(Integer subId) {
		this.subId = subId;
	}

	public String getBreviaryImage() {
		return breviaryImage;
	}

	public void setBreviaryImage(String breviaryImage) {
		this.breviaryImage = breviaryImage;
	}

	public String getSellerName() {
		return sellerName;
	}

	public void setSellerName(String sellerName) {
		this.sellerName = sellerName;
	}

	public String getSellerTradeName() {
		return sellerTradeName;
	}

	public void setSellerTradeName(String sellerTradeName) {
		this.sellerTradeName = sellerTradeName;
	}

	public Integer getConsumes() {
		return consumes;
	}

	public void setConsumes(Integer consumes) {
		this.consumes = consumes;
	}

	public String getZoneName() {
		return zoneName;
	}

	public void setZoneName(String zoneName) {
		this.zoneName = zoneName;
	}

	public String getComboName() {
		return comboName;
	}

	public void setComboName(String comboName) {
		this.comboName = comboName;
	}

	public BigDecimal getComboPrice() {
		return comboPrice;
	}

	public void setComboPrice(BigDecimal comboPrice) {
		this.comboPrice = comboPrice;
	}

	public BigDecimal getComboBird() {
		return comboBird;
	}

	public void setComboBird(BigDecimal comboBird) {
		this.comboBird = comboBird;
	}

	public String getPreTitle() {
		return preTitle;
	}

	public void setPreTitle(String preTitle) {
		this.preTitle = preTitle;
	}

	public String getLiverName() {
		return liverName;
	}

	public void setLiverName(String liverName) {
		this.liverName = liverName;
	}

	public Integer getSex() {
		return sex;
	}

	public void setSex(Integer sex) {
		this.sex = sex;
	}

	public Integer getIsFollow() {
		return isFollow;
	}

	public void setIsFollow(Integer isFollow) {
		this.isFollow = isFollow;
	}
	
	public Integer getRoomNo() {
		return roomNo;
	}

	public void setRoomNo(Integer roomNo) {
		this.roomNo = roomNo;
	}

	public String getStartDate() {
		return startDate;
	}

	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}
	
	public BigDecimal getFansPrice() {
		return fansPrice;
	}

	public void setFansPrice(BigDecimal fansPrice) {
		this.fansPrice = fansPrice;
	}

	@Override
	public String toString() {
		return "SpecilTopicSubInfoResponse [subId=" + subId + ", breviaryImage=" + breviaryImage + ", sellerName=" + sellerName
			+ ", sellerTradeName=" + sellerTradeName + ", consumes=" + consumes + ", zoneName=" + zoneName + ", comboName="
			+ comboName + ", comboPrice=" + comboPrice + ", comboBird=" + comboBird + ", preTitle=" + preTitle + ", liverName="
			+ liverName + ", sex=" + sex + ", isFollow=" + isFollow + ", roomNo=" + roomNo + ", startDate=" + startDate
			+ ", fansPrice=" + fansPrice + "]";
	}
	
}
