    <script type="text/javascript" src="<%=basePath %>js/jquery.js"></script>
    <script type="text/javascript" src="<%=basePath %>js/show.js"></script>
    <script>
        $(function(){
            $('.xmk-slidbtn').bind('click',function(){
                var par = $(this).parent().parent();
                var allow = par.find('.xmk-allowdown');
                var parContent = par.find('.xmk-xueyuan-qashow');
                var parTxt = par.find('.xmk-xueyuan-titxt');
                if(allow.hasClass('xmk-allowup')){
                    parTxt.show();
                    allow.removeClass('xmk-allowup');
                    parContent.slideUp();
                }else{
                    parTxt.hide();
                    allow.addClass('xmk-allowup');
                    parContent.slideDown();
                }
            });

            var tips = new showtips();

        /*     $('#moviecenter').bind('click',function(){
                tips.show('视频中心暂未开放，敬请期待');
            }); */

             $('#stulibbtn').bind('click',function(){
                tips.show('学习图书馆暂未开放，敬请期待');
            });
 

        });
    </script>
