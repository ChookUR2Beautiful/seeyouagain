package com.xmniao.xmn.core.coupon.response;

import java.io.Serializable;

/**
 * 
* @projectName: xmnService 
* @ClassName: CoupinCardInfoResponse    
* @Description:我的卡卷-充值卡返回类   
* @author: liuzhihao   
* @date: 2017年2月28日 上午11:38:29
 */
@SuppressWarnings("serial")
public class CouponCardInfoResponse implements Serializable{
	
	private String coinPrice;//可用鸟币额度
	
	private String sellerImage;//店铺图片
	
	private String sellerName;//店铺名称
	
	private String sellerId;//店铺ID
	
	private String sellerType;//店铺类型
	
	private String counts;//门店人次
	
	private String isLive;//是否存在直播 0 无  1 有
	
	private String cardDescription;//充值卡说明
	
	private String cardId;//充值卡ID

	public String getCoinPrice() {
		return coinPrice;
	}

	public void setCoinPrice(String coinPrice) {
		this.coinPrice = coinPrice;
	}

	public String getSellerImage() {
		return sellerImage;
	}

	public void setSellerImage(String sellerImage) {
		this.sellerImage = sellerImage;
	}

	public String getSellerName() {
		return sellerName;
	}

	public void setSellerName(String sellerName) {
		this.sellerName = sellerName;
	}
	
	public String getSellerId() {
		return sellerId;
	}

	public void setSellerId(String sellerId) {
		this.sellerId = sellerId;
	}

	public String getCounts() {
		return counts;
	}

	public void setCounts(String counts) {
		this.counts = counts;
	}

	public String getSellerType() {
		return sellerType;
	}

	public void setSellerType(String sellerType) {
		this.sellerType = sellerType;
	}
	
	public String getIsLive() {
		return isLive;
	}

	public void setIsLive(String isLive) {
		this.isLive = isLive;
	}

	public String getCardDescription() {
		return cardDescription;
	}

	public void setCardDescription(String cardDescription) {
		this.cardDescription = cardDescription;
	}

	public String getCardId() {
		return cardId;
	}

	public void setCardId(String cardId) {
		this.cardId = cardId;
	}

	@Override
	public String toString() {
		return "CouponCardInfoResponse [coinPrice=" + coinPrice + ", sellerImage=" + sellerImage + ", sellerName=" + sellerName
			+ ", sellerId=" + sellerId + ", sellerType=" + sellerType + ", counts=" + counts + ", isLive=" + isLive
			+ ", cardDescription=" + cardDescription + ", cardId=" + cardId + "]";
	}
	
}
