 */
@SuppressWarnings("serial")
public class AreaVersionRequest extends BaseRequest{

	private String areaversion;//区域更新版本号
	
	
	private Integer isAll;//全国城市必填

	public String getAreaversion() {
		return areaversion;
	}

	public void setAreaversion(String areaversion) {
		this.areaversion = areaversion;
	}
	public Integer getIsAll() {
		return isAll;
	}

	public void setIsAll(Integer isAll) {
		this.isAll = isAll;
	}
	
	
	
}
