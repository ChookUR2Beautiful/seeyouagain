
html {
  font-family: sans-serif; /* 1 */
  -ms-text-size-adjust: 100%; /* 2 */
  -webkit-text-size-adjust: 100%; /* 2 */
}

body {
  margin: 0;
  font-family: Microsoft Yahei;
}


article,
aside,
details, /* 1 */
figcaption,
figure,
footer,,
main, /* 2 */
menu,
nav,
section,
summary { /* 1 */
  display: block;
}

audio,
canvas,
progress,
video {
  display: inline-block;
}

audio:not([controls]) {
  display: none;
  height: 0;
}


progress {
  vertical-align: baseline;
}



template, /* 1 */
[hidden] {
  display: none;
}

a {
  background-color: transparent; /* 1 */
  -webkit-text-decoration-skip: objects; /* 2 */
}



a:active,
a:hover {
  outline-width: 0;
}


abbr[title] {
  border-bottom: none; /* 1 */
  text-decoration: underline; /* 2 */
  text-decoration: underline dotted; /* 2 */
}


b,
strong {
  font-weight: inherit;
}


b,
strong {
  font-weight: bolder;
}



dfn {
  font-style: italic;
}


h1 {
  font-size: 2em;
  margin: 0.67em 0;
}



mark {
  background-color: #ff0;
  color: #000;
}


small {
  font-size: 80%;
}


sub,
sup {
  font-size: 75%;
  line-height: 0;
  position: relative;
  vertical-align: baseline;
}

sub {
  bottom: -0.25em;
}

sup {
  top: -0.5em;
}


img {
  border-style: none;
}



svg:not(:root) {
  overflow: hidden;
}



code,
kbd,
pre,
samp {
  font-family: monospace, monospace; /* 1 */
  font-size: 1em; /* 2 */
}

figure {
  margin: 1em 40px;
}



hr {
  box-sizing: content-box; /* 1 */
  height: 0; /* 1 */
  overflow: visible; /* 2 */
}



button,
input,
optgroup,
select,
textarea {
  font: inherit; /* 1 */
  margin: 0; /* 2 */
}



optgroup {
  font-weight: bold;
}


button,
input { /* 1 */
  overflow: visible;
}



button,
select { /* 1 */
  text-transform: none;
}


button,
html [type="button"], /* 1 */
[type="reset"],
[type="submit"] {
  -webkit-appearance: button; /* 2 */
}


button::-moz-focus-inner,
[type="button"]::-moz-focus-inner,
[type="reset"]::-moz-focus-inner,
[type="submit"]::-moz-focus-inner {
  border-style: none;
  padding: 0;
}


button:-moz-focusring,
[type="button"]:-moz-focusring,
[type="reset"]:-moz-focusring,
[type="submit"]:-moz-focusring {
  outline: 1px dotted ButtonText;
}

fieldset {
  border: 1px solid #c0c0c0;
  margin: 0 2px;
  padding: 0.35em 0.625em 0.75em;
}



legend {
  box-sizing: border-box; /* 1 */
  color: inherit; /* 2 */
  display: table; /* 1 */
  max-width: 100%; /* 1 */
  padding: 0; /* 3 */
  white-space: normal; /* 1 */
}


textarea {
  overflow: auto;
}


[type="checkbox"],
[type="radio"] {
  box-sizing: border-box; /* 1 */
  padding: 0; /* 2 */
}

p{
  margin:0;
}



[type="number"]::-webkit-inner-spin-button,
[type="number"]::-webkit-outer-spin-button {
  height: auto;
}


[type="search"] {
  -webkit-appearance: textfield; /* 1 */
  outline-offset: -2px; /* 2 */
}



[type="search"]::-webkit-search-cancel-button,
[type="search"]::-webkit-search-decoration {
  -webkit-appearance: none;
}


::-webkit-input-placeholder {
  color: inherit;
  opacity: 0.54;
}



::-webkit-file-upload-button {
  -webkit-appearance: button; /* 1 */
  font: inherit; /* 2 */
}


.jobseekers{
  width: 100%;
  height: auto;

  padding:80px 10px 0;
  box-sizing: border-box;
}

.jobseekers .jobseekers-innerbox{
  width: 100%;
  height: auto;
  background: #fff;
  border-radius: 7px;
  text-align: center;
}

.jobseekers .jobseekers-innerbox .jobseekers-headpimg{
  width: 80px;
  height: 80px;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  border-radius: 50%;
  overflow: hidden;
  margin:0 auto 0;
  border:4px solid #fff;
  margin-top:-40px;
  display: inline-block;
}

.jobseekers-name{
  width: 90%;
  text-align: center;
  height: 40px;
  margin:0 auto;
  border-bottom: 2px dashed #dcdcdc;
}

.jobseekers-name-content{
  width: auto;
  height: 30px;
  line-height: 30px;
  color:#333;
  display: inline-block;
  vertical-align: middle;
}

.jobseekers-sex-f{
  display: inline-block;
  width: 20px;
  height: 20px;
  background-repeat: no-repeat;
  background-image: url(../img/recurit_sex_f.png);
  vertical-align: middle;
}

.jobseekers-info{
  width: 100%;
  height: auto;
  text-align: center;
  margin-top:10px;
  font-size:0px;
  padding-bottom: 18px;
}

.jobseekers-info-yearsold{
  padding:0 6px;
  text-align: center;
  height: 30px;
  display: inline-block;
  line-height: 30px;
  color:#666;
  font-size: 14px;
}

.jobseekers-info-eduimg{
  display: inline-block;
  width: 20px;
  height: 20px;
  background-repeat: no-repeat;
  background-image: url(../img/recurit_work_education.png);
  background-size: contain;
  background-position: center;
  vertical-align: middle;
  margin:-10px 2px 0;
}

.jobseekers-info-normalinfo{
  padding:0 2px;
  text-align: center;
  height: 30px;
  display: inline-block;
  line-height: 30px;
  color:#666;
  font-size: 14px;
}

.jobseekers-info-workyear{
  display: inline-block;
  width: 20px;
  height: 20px;
  background-repeat: no-repeat;
  background-image: url(../img/recurit_work_year.png);
  background-size: contain;
  background-position: center;
  vertical-align: middle;
  margin:-10px 2px 0;
}

.jobseekers-info-workplace{
  display: inline-block;
  width: 20px;
  height: 20px;
  background-repeat: no-repeat;
  background-image: url(../img/recurit_work_city.png);
  background-size: contain;
  background-position: center;
  vertical-align: middle;
  margin:-10px 2px 0;
}

.box-reset-size{
  padding:0 10px;
  margin-top:18px;
  margin-bottom: 25px;
}

.jobseekers-wantdo{
  width: 90%;
  height: auto;
  margin: 0 auto;
  
 margin-bottom: -6px;
  text-align: left;
  font-size: 0px;
}

.jobseekers-wantdo:first-child{
  border-bottom: 1px solid #ccc;
}

.jobseekers-wantdo-title{
  width: 33%;
  display: inline-block;
  height: 30px;
  line-height: 30px;
  text-align: left;
  padding:20px 0px;
  font-size: 14px;
  vertical-align: top;
}

.jobseekers-wantdo-tic{
  width: auto;
  display: inline-block;
  height: 30px;
  line-height: 30px;
}

.jobseekers-wantdo-img{
  display: inline-block;
  width: 25px;
  height: 25px;
  background-repeat: no-repeat;
  background-image: url(../img/job_iwant_tobe.png);
  background-size: contain;
  vertical-align: middle;
}

.jobseekers-myworked-img{
  display: inline-block;
  width: 25px;
  height: 25px;
  background-repeat: no-repeat;
  background-image: url(../img/job_my_worked.png);
  background-size: contain;
  vertical-align: middle;
}

.jobseekers-wantdo-label{
  width: 67%;
  display: inline-block;
  font-size: 14px;
  word-break: break-all;
  padding: 0;
  margin-top: 23px;
  font-size: 0px;
}

.jobseekers-wantdo-label li{
  display: inline-block;
  font-size: 14px;
  color:#f6ab00;
  padding:2px 8px;
  border:1px solid #f6ab00;
  border-radius: 4px;
  margin-right: 20px;
  margin-bottom: 14px;
}

.jobseekers-intro{
  width: 100%;
  height: auto;
  padding-bottom: 50px;
  background: #fff;
  padding-top:10px;
}

.jobseekers-intro-title{
  width: 100%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  margin:0px auto;
  color:#666;
  position: relative;
}

.jobseekers-intro-title:before{
  content:"";
  position: absolute;
  top:13px;
  left: 8%;
  width: 30%;
  height: 1px;
  background: #dcdcdc;
}

.jobseekers-intro-title:after{
  content:"";
  position: absolute;
  top:13px;
  right: 8%;
  width: 30%;
  height: 1px;
  background: #dcdcdc;
}

.jobseekers-intro-label{
  padding:0 8%;
  margin:0 auto;
  width: 100%;
  box-sizing: border-box;
  font-size: 0px;
  margin-top:10px;

}

.jobseekers-intro-label li{
  display: inline-block;
  padding:2px 7px;
  color:#f6ab00;
  border:1px solid #f6ab00;
  border-radius: 5px;
  font-size: 14px;
   margin-right:18px;
  margin-bottom: 13px;
}

/*求职者详情end*/


/*商家详情*/

.business-box{
  width: 100%;
  padding:0 10px;
  box-sizing: border-box;
  margin-top:20px;

}

.business-innerbox{
  width: 100%;
  height: auto;
  background: #fff;
   border-radius: 5px;
  padding: 15px 10px;
  box-sizing: border-box;
}
.business-innerbox-frame{
  width: 100%;
  height: 32px;
  border-bottom: 2px dashed #dcdcdc;
}

.business-innerbox-workname{
  width: auto;
  float:left;
  font-size: 16px;
  color: #333;
  font-weight: bold;
}

.business-innerbox-salary{
  float:right;
  width: auto;
  font-size: 16px;
  color:#ff5766;

}

.resetjobseekersinfo{
  text-align: left;
}

.resetjobseekersinfo .jobseekers-info-yearsold{
  padding:0 10px;
}

.resetjobseekerslabel{
  margin:0;
  padding:0;
}

.business-shops-info{
  width: 100%;
  height: auto;
  background: #fff;
  margin-top:20px;
}

.business-shops-img{
  width: 100%;
  height: 180px;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;

}

.business-shops-name{
  width: 100%;
  height: 60px;
  line-height: 60px;
  color:#333;
  text-align: center;
  font-size: 20px;
}

.business-position-info{
  width: 100%;
  height: auto;
  background: #fff;
  margin-top:20px;
  box-sizing: border-box;
  padding:10px 20px 80px;
}

.business-position-innercontent{
  height: auto;
  
}

.business-position-innercontent .business-position-rename{
  font-size: 14px;
  color:#999;
  line-height: 1.7em;
}

.business-position-place{
  display: inline-block;
  width: 20px;
  height: 20px;
  background-repeat: no-repeat;
  background-size: contain;
  background-image: url(../img/recurit_work_city.png);
  background-position: center;
  vertical-align: middle;
  margin-top:-2px;
  margin-right: 5px;
}

.business-position-otherp{
  width: 100%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  font-size: 14px;
  color:#666;
  margin-top:15px;
  position: relative;
}
.business-position-otherp:after{
  content: "";
  position: absolute;
  top:14px;
  left: 0;
  width: 33%;
  height: 1px;
  background: #dcdcdc;
}

.business-position-otherp:before{
  content: "";
  position: absolute;
  top:14px;
  right: 0;
  width: 33%;
  height: 1px;
  background: #dcdcdc;
}

.positionresetjobseekers{
  margin-top:10px;
}
