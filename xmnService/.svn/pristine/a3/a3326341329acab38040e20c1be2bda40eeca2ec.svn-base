package com.xmniao.xmn.core.live.entity;

import java.io.Serializable;
import java.util.Date;


/**
 * 预告/直播列表记录获取
 * @author yhl
 * 2016年8月11日10:33:11
 * */
public class LiveAnchorInfo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 5593406973711000328L;
	
	//直播记录id',
	private Long id;
	
	//主播用户id',
	private Long anchor_id ;
	
	// 用户昵称',
	private String nname;
	
	// 头像',
	private String avatar;
	
	//主播房间编号',
	private Long anchor_room_no ;
	
	//直播商家ID',;
	private Long sellerid  ;
	
	// 直播商家名称',
	private String sellername;
	
	//直播商家所在城市',
	private String seller_city  ;
	
	//是否推荐的商家  1 推荐  0 不是推荐',
	private Integer seller_status ;
	
	//直播地址',
	private String zhibo_address ;
	
	//'直播类型  0 预告 1 正在直播  2 回放  3 不回放',
	private Integer zhibo_type ;
	
	//  回放地址URL',
	private String zhibo_playback_url;
	
	//直播/回放标题',
	private String zhibo_title ;
	
	// 直播/回放封面',
	private String zhibo_cover;
	
	//直播计划开始时间',
	private Date plan_start_date ;
	
	//直播计划结束时间',
	private Date plan_end_date ;
	
	// 直播开始时间',
	private Date start_date;
	
	//直播结束时间',;
	private Date end_date ;
	
	//直播时长',
	private Date zhibo_duration ;
	
	// 默认 1启用   0停用',
	private Integer status ;
	
	// 加入时间',
	private Date create_time;
	
	// 更新时间',
	private Date update_time;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getAnchor_id() {
		return anchor_id;
	}

	public void setAnchor_id(Long anchor_id) {
		this.anchor_id = anchor_id;
	}

	public String getNname() {
		return nname;
	}

	public void setNname(String nname) {
		this.nname = nname;
	}

	public String getAvatar() {
		return avatar;
	}

	public void setAvatar(String avatar) {
		this.avatar = avatar;
	}

	public Long getAnchor_room_no() {
		return anchor_room_no;
	}

	public void setAnchor_room_no(Long anchor_room_no) {
		this.anchor_room_no = anchor_room_no;
	}

	public Long getSellerid() {
		return sellerid;
	}

	public void setSellerid(Long sellerid) {
		this.sellerid = sellerid;
	}

	public String getSellername() {
		return sellername;
	}

	public void setSellername(String sellername) {
		this.sellername = sellername;
	}

	public String getSeller_city() {
		return seller_city;
	}

	public void setSeller_city(String seller_city) {
		this.seller_city = seller_city;
	}

	public Integer getSeller_status() {
		return seller_status;
	}

	public void setSeller_status(Integer seller_status) {
		this.seller_status = seller_status;
	}

	public String getZhibo_address() {
		return zhibo_address;
	}

	public void setZhibo_address(String zhibo_address) {
		this.zhibo_address = zhibo_address;
	}

	public Integer getZhibo_type() {
		return zhibo_type;
	}

	public void setZhibo_type(Integer zhibo_type) {
		this.zhibo_type = zhibo_type;
	}

	public String getZhibo_playback_url() {
		return zhibo_playback_url;
	}

	public void setZhibo_playback_url(String zhibo_playback_url) {
		this.zhibo_playback_url = zhibo_playback_url;
	}

	public String getZhibo_title() {
		return zhibo_title;
	}

	public void setZhibo_title(String zhibo_title) {
		this.zhibo_title = zhibo_title;
	}

	public String getZhibo_cover() {
		return zhibo_cover;
	}

	public void setZhibo_cover(String zhibo_cover) {
		this.zhibo_cover = zhibo_cover;
	}

	public Date getPlan_start_date() {
		return plan_start_date;
	}

	public void setPlan_start_date(Date plan_start_date) {
		this.plan_start_date = plan_start_date;
	}

	public Date getPlan_end_date() {
		return plan_end_date;
	}

	public void setPlan_end_date(Date plan_end_date) {
		this.plan_end_date = plan_end_date;
	}

	public Date getStart_date() {
		return start_date;
	}

	public void setStart_date(Date start_date) {
		this.start_date = start_date;
	}

	public Date getEnd_date() {
		return end_date;
	}

	public void setEnd_date(Date end_date) {
		this.end_date = end_date;
	}

	public Date getZhibo_duration() {
		return zhibo_duration;
	}

	public void setZhibo_duration(Date zhibo_duration) {
		this.zhibo_duration = zhibo_duration;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Date getCreate_time() {
		return create_time;
	}

	public void setCreate_time(Date create_time) {
		this.create_time = create_time;
	}

	public Date getUpdate_time() {
		return update_time;
	}

	public void setUpdate_time(Date update_time) {
		this.update_time = update_time;
	}

	@Override
	public String toString() {
		return "LiveAnchorInfo [id=" + id + ", anchor_id=" + anchor_id
				+ ", nname=" + nname + ", avatar=" + avatar
				+ ", anchor_room_no=" + anchor_room_no + ", sellerid="
				+ sellerid + ", sellername=" + sellername + ", seller_city="
				+ seller_city + ", seller_status=" + seller_status
				+ ", zhibo_address=" + zhibo_address + ", zhibo_type="
				+ zhibo_type + ", zhibo_playback_url=" + zhibo_playback_url
				+ ", zhibo_title=" + zhibo_title + ", zhibo_cover="
				+ zhibo_cover + ", plan_start_date=" + plan_start_date
				+ ", plan_end_date=" + plan_end_date + ", start_date="
				+ start_date + ", end_date=" + end_date + ", zhibo_duration="
				+ zhibo_duration + ", status=" + status + ", create_time="
				+ create_time + ", update_time=" + update_time + "]";
	}

}
