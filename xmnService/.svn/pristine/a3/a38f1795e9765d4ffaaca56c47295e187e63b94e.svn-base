package com.xmniao.xmn.core.market.entity.home;

import com.xmniao.xmn.core.market.entity.pay.FreshActivityCommon;

import java.util.Date;

public class FreshModule {
    private Long id;

    private String moduleName;

    private Integer moduleType;

    private Integer productType;

    private Long typeId;

    private Long activityId;

    private Integer showNum;

    private Integer state;

    private Integer sort;

    private Date createTime;

    private Date updateTime;

    private String imageUrl;

    private String jumpUrl;

    private Integer activityProductType;
    private Date endTime;


    private Long distEntTime;

    public Long getDistEntTime() {
        return distEntTime;
    }

    public void setDistEntTime(Long distEntTime) {
        this.distEntTime = distEntTime;
    }

    public String getJumpUrl() {
        return jumpUrl;
    }

    public void setJumpUrl(String jumpUrl) {
        this.jumpUrl = jumpUrl;
    }

    public Integer getActivityProductType() {
        return activityProductType;
    }

    public void setActivityProductType(Integer activityProductType) {
        this.activityProductType = activityProductType;
    }

    public String getImageUrl() {

        return imageUrl;
    }

    public void setImageUrl(String imageUrl) {
        this.imageUrl = imageUrl;
    }

    private FreshActivityCommon activity;

    public FreshActivityCommon getActivity() {
        return activity;
    }

    public void setActivity(FreshActivityCommon activity) {
        this.activity = activity;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getModuleName() {
        return moduleName;
    }

    public void setModuleName(String moduleName) {
        this.moduleName = moduleName == null ? null : moduleName.trim();
    }

    public Integer getModuleType() {
        return moduleType;
    }

    public void setModuleType(Integer moduleType) {
        this.moduleType = moduleType;
    }

    public Integer getProductType() {
        return productType;
    }

    public void setProductType(Integer productType) {
        this.productType = productType;
    }

    public Long getTypeId() {
        return typeId;
    }

    public void setTypeId(Long typeId) {
        this.typeId = typeId;
    }

    public Long getActivityId() {
        return activityId;
    }

    public void setActivityId(Long activityId) {
        this.activityId = activityId;
    }

    public Integer getShowNum() {
        return showNum;
    }

    public void setShowNum(Integer showNum) {
        this.showNum = showNum;
    }

    public Integer getState() {
        return state;
    }

    public void setState(Integer state) {
        this.state = state;
    }

    public Integer getSort() {
        return sort;
    }

    public void setSort(Integer sort) {
        this.sort = sort;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

    public void setEndTime(Date endTime) {
        this.endTime = endTime;
    }

    public Date getEndTime() {
        return endTime;
    }

    @Override
    public String toString() {
        return "FreshModule{" +
            "id=" + id +
            ", moduleName='" + moduleName + '\'' +
            ", moduleType=" + moduleType +
            ", productType=" + productType +
            ", typeId=" + typeId +
            ", activityId=" + activityId +
            ", showNum=" + showNum +
            ", state=" + state +
            ", sort=" + sort +
            ", createTime=" + createTime +
            ", updateTime=" + updateTime +
            ", imageUrl='" + imageUrl + '\'' +
            ", jumpUrl='" + jumpUrl + '\'' +
            ", activityProductType=" + activityProductType +
            ", endTime=" + endTime +
            ", distEntTime=" + distEntTime +
            ", activity=" + activity +
            '}';
    }
}