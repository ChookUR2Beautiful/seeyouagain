package com.xmniao.xmn.core.live.entity;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;


/**
 * 直播礼物信息类
 * @author yhl
 * 2016年8月10日17:23:49
 * */
public class LiveGivedGiftInfo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 6650464273838935486L;
	
	// 直播礼物id
	private Long id ;
	
	//赠送记录唯一序列号 
	private String givedGiftKey;
	
	//直播记录id 
	private Long liveRecordId;
	
	//观看直播的用户ID
	private Long liverId;
	
	//钱包扣款记录ID
	private Integer walletRecordId;
	
	//直播用户类型： 1 主播 2 普通用户
	private Long liverUtype;
	
	//主播用户id
	private Long anchorId;
	
	//主播房间编号
	private String  anchorRoomNo;
	
	//礼物id
	private Integer giftId;
	
	//礼物名称
	private String giftName;
	
	//礼物个数
	private Integer giftNums;
	
	//礼物单价（单位鸟币 , 积分）
	private BigDecimal giftPrice;
	
	//礼物代表的经验值
	private Integer giftExperience;
		
	//1 扣款成功  2 扣款失败
	private Integer status;
	
	//预处理状态  1 未接受  2 预接收 3 已接收
	private Integer advancedStatus;
	
	private Date createTime;
	
	private Date updateTime;
	
	//接收状态
	private Integer isfailed;
	
	//分账鸟蛋
	private BigDecimal percentAmount;
	
	//套餐礼物 , 商城礼物对应的ID
	private Integer saleProductId;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getGivedGiftKey() {
		return givedGiftKey;
	}

	public void setGivedGiftKey(String givedGiftKey) {
		this.givedGiftKey = givedGiftKey;
	}

	public Long getLiveRecordId() {
		return liveRecordId;
	}

	public void setLiveRecordId(Long liveRecordId) {
		this.liveRecordId = liveRecordId;
	}

	public Long getLiverId() {
		return liverId;
	}

	public void setLiverId(Long liverId) {
		this.liverId = liverId;
	}

	public Integer getWalletRecordId() {
		return walletRecordId;
	}

	public void setWalletRecordId(Integer walletRecordId) {
		this.walletRecordId = walletRecordId;
	}

	public Long getLiverUtype() {
		return liverUtype;
	}

	public void setLiverUtype(Long liverUtype) {
		this.liverUtype = liverUtype;
	}

	public Long getAnchorId() {
		return anchorId;
	}

	public void setAnchorId(Long anchorId) {
		this.anchorId = anchorId;
	}

	public String getAnchorRoomNo() {
		return anchorRoomNo;
	}

	public void setAnchorRoomNo(String anchorRoomNo) {
		this.anchorRoomNo = anchorRoomNo;
	}

	public Integer getGiftId() {
		return giftId;
	}

	public void setGiftId(Integer giftId) {
		this.giftId = giftId;
	}

	public String getGiftName() {
		return giftName;
	}

	public void setGiftName(String giftName) {
		this.giftName = giftName;
	}

	public Integer getGiftNums() {
		return giftNums;
	}

	public void setGiftNums(Integer giftNums) {
		this.giftNums = giftNums;
	}

	public BigDecimal getGiftPrice() {
		return giftPrice;
	}

	public void setGiftPrice(BigDecimal giftPrice) {
		this.giftPrice = giftPrice;
	}

	public Integer getGiftExperience() {
		return giftExperience;
	}

	public void setGiftExperience(Integer giftExperience) {
		this.giftExperience = giftExperience;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getAdvancedStatus() {
		return advancedStatus;
	}

	public void setAdvancedStatus(Integer advancedStatus) {
		this.advancedStatus = advancedStatus;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	
	public Integer getIsfailed() {
		return isfailed;
	}

	public void setIsfailed(Integer isfailed) {
		this.isfailed = isfailed;
	}
	
	public BigDecimal getPercentAmount() {
		return percentAmount;
	}

	public void setPercentAmount(BigDecimal percentAmount) {
		this.percentAmount = percentAmount;
	}
	
	public Integer getSaleProductId() {
		return saleProductId;
	}

	public void setSaleProductId(Integer saleProductId) {
		this.saleProductId = saleProductId;
	}

	@Override
	public String toString() {
		return "LiveGivedGiftInfo [id=" + id + ", givedGiftKey=" + givedGiftKey
				+ ", liveRecordId=" + liveRecordId + ", liverId=" + liverId
				+ ", walletRecordId=" + walletRecordId + ", liverUtype="
				+ liverUtype + ", anchorId=" + anchorId + ", anchorRoomNo="
				+ anchorRoomNo + ", giftId=" + giftId + ", giftName="
				+ giftName + ", giftNums=" + giftNums + ", giftPrice="
				+ giftPrice + ", giftExperience=" + giftExperience
				+ ", status=" + status + ", advancedStatus=" + advancedStatus
				+ ", createTime=" + createTime + ", updateTime=" + updateTime
				+ ", isfailed=" + isfailed + ", percentAmount=" + percentAmount
				+ ", saleProductId=" + saleProductId + "]";
	}



}
