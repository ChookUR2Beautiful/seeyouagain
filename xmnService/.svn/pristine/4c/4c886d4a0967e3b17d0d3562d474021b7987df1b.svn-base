.perfectinfo-list{
	width: 100%;
	padding:0 15px;
	box-sizing: border-box;
}

.perfectinfo-list li{
	width: 100%;
	border-bottom: 1px solid #f2f2f2;
	padding:20px 0;
}

.perfectinfo-list li i{
	display: inline-block;
	width: 18px;
	height: 18px;
	background-repeat: no-repeat;
	background-position: center;
	background-size: contain;
	vertical-align: middle;
	margin-right: 3px;
}

.perfectinfo-list li .perfectinfo-name{
	background-image: url(../img/sign_user_name.png);
}
.perfectinfo-list li .perfectinfo-email{
	background-image: url(../img/sign_e_mail.png);
}
.perfectinfo-list li .perfectinfo-wx{
	background-image: url(../img/sign_weixin_name.png);
}

.perfectinfo-list li span{
	display: inline-block;
	vertical-align: middle;
	font-size: 15px;
	color:#333;
}

.perfectinfo-list li .perfectinfo-formtext{
	display: inline-block;
	vertical-align: middle;
	border-radius: 5px;
	background: #fff;
	outline: none;
	border:1px solid #ccc;
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	padding:0 10px;
}

.perfectinfo-list li .perfectinfo-formtext:focus{
	border:1px solid #f6ab00;
}

/*支付选择页面*/
.sign-pay-title{
	height: 40px;
	line-height: 40px;
	padding:0 20px;
	color:#999;
}

.sign-type-list{
	width: 100%;
	height: auto;
	box-sizing: border-box;
	padding:0px 10px 0;
	font-size: 0px;
	
}

.sign-type-list li{
	display: inline-block;
	width: 33.33333%;
	height: auto;
	padding-bottom: 22px;
	text-align: center;
	border-bottom: 1px solid #efefef;
}

.sign-type-list li .sign-type-num{
	padding:0 5px;
	height: 50px;
	min-width: 50px;
	display: inline-block;
	background: #dddddd;
	font-size: 14px;
	border-radius: 12px;
	margin:0 auto;
	vertical-align: top;
}

.sign-type-list li a{
	
} 



.sign-type-list li .sign-type-num .sign-type-val{
	font-size:30px;
	display: inline-block;
	margin-top:5px;
	vertical-align: middle;
	color:#fff;
}

.sign-type-list li .sign-type-num .sign-type-unit{
	font-size: 14px;
	display: inline-block;
	vertical-align: middle;
	color:#fff;
}

.sign-type-list li .sign-type-money{
	width: 100%;
	margin-top:5px;
	line-height: 1.5em;
	font-size: 15px;
	color:#ddd;
}

.sign-type-list li .sign-type-soucemoney{
	color:#4a4a4a;
	font-size: 14px;
	margin-top:5px;
}

.sign-type-list li .sign-type-perselect{
	color:#ddd;
	font-size: 14px;
	margin-top: 3px;
}

.sign-type-list li .sign-type-perselect span{
	color:#4a4a4a;
}

.sign-type-list .active .sign-type-num{
	background: #f7ab00;
}

.sign-type-list .active .sign-type-money{
	color:#f7ab00;
}

.sign-type-chec{
	padding:40px 0;
}

.sign-type-chec .sign-type-chec-local{
	text-align: center;
	font-size: 15px;
	color:#333;
	line-height: 1.5em;
}

.sign-type-chec .sign-type-chec-total , .sign-type-chec .sign-type-chec-discount{
	text-align: center;
	font-size: 15px;
	color:#ddd;
	line-height: 1.5em;
}

.sign-type-line{
	display: block;width: 100%;
	height: 20px;
	background: #f6f6f6;
}

.sign-type-pay{
	display: block;
	width: 100%;
	height: auto;
}

.sign-type-pay .sign-type-paybtn{
	width: 100px;
	height: 100px;
	margin:10px auto;
	border-radius: 9px;
	display: block;
	padding-top: 10px;
}

.sign-type-wx{
	display: block;
	width: 50px;
	height: 50px;
	background-repeat: no-repeat;
	background-size: contain;
	background-position: center;
	background-image: url(../img/sign_weixin_pay.png);
	margin:10px auto;
}

.sign-type-pay .sign-type-paybtn p{
	text-align: center;
	font-size: 16px;
	color:#999;
}


/*邀请好友加入*/
.invitef-title{
text-align: center;
height: 40px;
line-height: 40px;
color:#333;
margin-top:15px;
font-size: 18px;
}
.invitef-img{
	height: 80px;
	margin-top:10px;
	width: 100%;
	text-align: center;
}

.invitef-img img{
	height: 100%;
	width: auto;
}

.invitef-p{
	color:#f6ab00;
	text-align: center;
	margin-top: 10px;
}

.invitef-info-list{
	word-break: break-all;
	padding:10px 20px 0 40px;
	color:#999;
	font-size: 15px;
	line-height: 1.5em;
}

.addcreateman-p{
	width: 100%;
	color:#4f96dc;
	word-break: break-all;
	text-align: center;
	margin-top: 10px;
}

.addcreateman-intro{
	text-indent: 2em;
