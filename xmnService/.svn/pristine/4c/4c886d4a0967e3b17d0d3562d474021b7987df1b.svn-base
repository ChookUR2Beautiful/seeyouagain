@charset "UTF-8";
html{
	margin:0;
	padding:0;
	width: 100%;
	height: 100%;
}
body{
	font-family: Microsoft Yahei;
	padding:0;
	margin:0;
	
}

a{
	text-decoration: none;
	color:#333;
}

a:focus{
	text-decoration: none;
	color:#333;
}

p{
	margin:0;
	padding:0;
}

ul{
	list-style-type: none;
	margin:0;
	padding:0;
}

/*签约成功*/
.paysuccess-img{
	width: 100%;
	height: 100px;
	text-align: center;
	margin-top:50px;
}

.paysuccess-img img{
	height: 100px;
	width: auto;
}

.paysuccess-tips{
	text-align: center;
	height: 40px;
	line-height: 40px;
	color:#333;
	font-size: 16px;

}

.paysuccess-cc{
	font-size: 14px;
	color:#999;padding:10px 20px;
}

.paysuccess-text-center{
	text-align: center;

}


/*end*/


/*支付方式*/
.paytype{
	background: #f6f6f6;
}


.paytype-row{
	width: 100%;
	height: 60px;
	line-height: 60px;
	padding:0 20px;
	box-sizing: border-box;
	background:#fff;
}

.paytype-money{
	width: 100%;
	text-align:left;
}

.paytype-money-title{
	color:#333;
	font-size: 16px;
}

.paytype-money-val{
	font-size: 22px;
	color:#f6ab00;
}

.paytype-button{
	width: 100%;
	margin-top:20px;
	height: 110px;
	background:#fff;
	padding-top: 20px;
	text-align: center;
}

.paytype-button a{
	display: block;
	width: 80px;
	height: 70px;
	border:1px solid #e8e8e8;
	margin:0 auto;
	border-radius: 5px;
	padding-top: 10px;
}

.paytype-button a span{
	width: 40px;
	height: 40px;
	text-align: center;
	display: block;
	margin:0 auto;
	background-image: url(../img/sign_weixin_pay.png);
	background-repeat: no-repeat;
	background-size: contain;
}

.paytype-button a p{
	width: 100%;
	text-align: center;font-size: 14px;
	height: 30px;
	line-height: 30px;
}

/*支付确认*/

.payconfirm{
	background: #f6f6f6;
	padding-bottom: 150px;
}

.payconfirm-val{
	width: 100%;
	height: 80px;
	background: #f7b734;
	padding-top: 23px;
	box-sizing: border-box;
}

.payconfirm-p{
	width: 100%;
	text-align: center;
	color:#fff;
	font-size: 14px;
}

.payconfirm-p span{
	font-size: 25px;
	margin-left: 5px;
}

.payconfirm-tips{
	padding:12px 15px;
	text-align: center;
	background: #fff;

}

.payconfirm-tips p{
	font-size: 15px;
	line-height: 1.5em;
	color:#666;
}

.payconfirm-info-list{
	margin-top:20px;
}

.payconfirm-info-list li{
	display: block;
	/*width: 100%;*/
	/*height: 60px;*/
	padding:20px 15px;
	/*box-sizing: border-box;*/
	background: #fff;
	/*line-height: 60px;*/
	border-bottom: 1px solid #f2f2f2;
}

.payconfirm-info-list li i{
	display: inline-block;
	width: 15px;
	height: 15px;
	background-repeat: no-repeat;
	background-position:center;
	background-size: contain;
	vertical-align: middle;
	margin-right: 5px;
}

.payconfirm-info-list li .payconfirm-info-shopsname{
	background-image: url(../img/shops_sign_shopname.png);
}

.payconfirm-info-list li .payconfirm-info-address{
	background-image: url(../img/shopi_shop_place.png);
}

.payconfirm-info-list li .payconfirm-info-principal{
	background-image: url(../img/shopi_shop_principal.png);
}

.payconfirm-info-list li .payconfirm-info-leadingofficial{
	background-image: url(../img/shops_sign_leadingofficial.png);
}

.payconfirm-info-list li .payconfirm-info-discountset{
	background-image: url(../img/shops_sign_settingdiscount.png);
}

.payconfirm-info-list li .payconfirm-info-signtime{
	background-image: url(../img/shopi_shop_openinghours.png);
}





.payconfirm-info-list span{
	font-size: 15px;
	/*display: inline-block;*/
}

.payconfirm-info-list span.payconfirm-info-title{
	color:#666;
}

.payconfirm-info-list span.payconfirm-info-val{
	color:#999;
}


/**/

.bottomfixbtn{
	width: 100%;
	height: 60px;
	background: #f6ab00;
	display: block;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 99;
	text-align: center;
	line-height: 60px;
	color:#fff;
	font-size: 17px;
}

.bottomfixbtn:focus{
	color:#fff;
}

/*签约支付成功*/

.signsuccess-img{
	width: 100%;
	height: 100px;
	margin-top:50px;
	text-align: center;
}

.signsuccess-img img{
	width: auto;
	height: 100px;
}

.signsuccess-tips{
	text-align: center;
	height: 40px;
	line-height: 40px;
	color:#333;
	font-size: 16px;
}

.signsuccess-tipsc{
	text-align: center;
	height: 30px;
	line-height: 30px;
	margin-top:15px;
	font-size: 14px;
	color:#999;
}

/*支付失败*/

.signfail-tips{
	text-align: left;
	padding:0 15px;
	font-size: 14px;
	color:#999;
}

.signfail-tips-c{
	text-align: center;
	padding:0 15px;
	font-size: 14px;
	color:#999;
	margin-top:15px;
}

.doublefixbtn{
	width: 100%;
	font-size: 0px;
	position: fixed;
	bottom: 0;
	left: 0;
	height: 60px;
	background: #fff;

}

.doublefixbtn .doublefixbtn-firstbtn{
	display: inline-block;
	width: 50%;
	height: 100%;
	background: #f5f6f8;
	font-size: 17px;
	text-align: center;line-height: 60px;
	color:#999;
}

.doublefixbtn .doublefixbtn-secondbtn{
	display: inline-block;
	width: 50%;
	height: 100%;
	background: #f6ab00;
	font-size: 17px;
	text-align: center;line-height: 60px;
	color:#fff;
}

/*完善资料*/
.perfectinfo{
	padding-bottom: 150px;
}

.perfectinfo-hrep{
	width: 100%;
	height: 150px;
	border-bottom: 1px solid #f2f2f2;
	padding-top:30px;
	box-sizing: border-box;
}

.perfectinfo-hrep .uploadimage{
	display: block;
	width: 80px;
	height: 80px;
	margin:0 auto;
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center;
	border-radius: 50%;
	line-height: 80px;
	text-align: center;
	font-size: 14px;
	color:#666;
	position: relative;
}
.perfectinfo-hrep .uploadimage #imgelector{
	display: block;
	position: absolute;
	bottom: -30px;
	left: 0;
	width: 100%;
	text-align: center;
	color: #333;
}

