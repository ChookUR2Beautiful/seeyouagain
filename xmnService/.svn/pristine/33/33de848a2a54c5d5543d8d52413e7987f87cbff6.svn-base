package com.xmniao.xmn.core.common.request.vod;

import net.sf.oval.constraint.NotNull;

import com.xmniao.xmn.core.base.BaseRequest;


/**
 * 
* 类名称：MultipartUploadVodFileRequest   
* 类描述：上传视频请求参数   
* 创建人：xiaoxiong   
* 创建时间：2016年8月26日 上午9:07:27   
* 修改人：xiaoxiong   
* 修改时间：2016年8月26日 上午9:07:27   
* 修改备注：   
* @version    
*
 */
public class MultipartUploadVodFileRequest extends BaseRequest{
	
	@NotNull(message="本地文件路径不能为空")
	private String file;
	@NotNull(message="文件名称不能为空")
	private String filename;
	@NotNull(message="文件sha不能为空")
	private String filesha;
	@NotNull(message="文件大小不能为空")
	private Long filesize;
	@NotNull(message="")
	private Integer datesize;
	@NotNull(message="")
	private Integer offset;
	@NotNull(message="")
	private String filetype;
	
	private String tags;
	
	private Integer classid;
	
	private Integer istranscode;
	
	private Integer isscreenshot;
	
	private Integer iswatermark;
	
	

	public String getFile() {
		return file;
	}

	public void setFile(String file) {
		this.file = file;
	}

	public String getFilename() {
		return filename;
	}

	public void setFilename(String filename) {
		this.filename = filename;
	}

	public String getFilesha() {
		return filesha;
	}

	public void setFilesha(String filesha) {
		this.filesha = filesha;
	}


	public Long getFilesize() {
		return filesize;
	}

	public void setFilesize(Long filesize) {
		this.filesize = filesize;
	}

	public Integer getDatesize() {
		return datesize;
	}

	public void setDatesize(Integer datesize) {
		this.datesize = datesize;
	}

	public Integer getOffset() {
		return offset;
	}

	public void setOffset(Integer offset) {
		this.offset = offset;
	}

	public String getFiletype() {
		return filetype;
	}

	public void setFiletype(String filetype) {
		this.filetype = filetype;
	}

	public String getTags() {
		return tags;
	}

	public void setTags(String tags) {
		this.tags = tags;
	}

	public Integer getClassid() {
		return classid;
	}

	public void setClassid(Integer classid) {
		this.classid = classid;
	}

	public Integer getIstranscode() {
		return istranscode;
	}

	public void setIstranscode(Integer istranscode) {
		this.istranscode = istranscode;
	}

	public Integer getIsscreenshot() {
		return isscreenshot;
	}

	public void setIsscreenshot(Integer isscreenshot) {
		this.isscreenshot = isscreenshot;
	}

	public Integer getIswatermark() {
		return iswatermark;
	}

	public void setIswatermark(Integer iswatermark) {
		this.iswatermark = iswatermark;
	}
	
	
	
	
	
	
}
