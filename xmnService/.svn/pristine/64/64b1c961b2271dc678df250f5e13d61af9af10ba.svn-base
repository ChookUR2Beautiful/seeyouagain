package com.xmniao.xmn.core.seller.entity;

import java.util.Date;

/**
 * 
* @projectName: xmnService 
* @ClassName: DebitcardSeller    
* @Description:充值卡实体   
* @author: liuzhihao   
* @date: 2017年2月23日 上午11:32:58
 */
public class DebitcardSeller {
    private Integer id;

    private Integer sellerid;

    private Integer sellertype;

    private String sellername;

    private Integer status;

    private String subSellerid;

    private Date updateTime;
    
    private double totalLimitRecharge;
    
    private String rechargeItem;
    
    private double referrerRatio;
    
    private double upparenReferrerRatio;
    

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getSellerid() {
        return sellerid;
    }

    public void setSellerid(Integer sellerid) {
        this.sellerid = sellerid;
    }

    public Integer getSellertype() {
        return sellertype;
    }

    public void setSellertype(Integer sellertype) {
        this.sellertype = sellertype;
    }

    public String getSellername() {
        return sellername;
    }

    public void setSellername(String sellername) {
        this.sellername = sellername == null ? null : sellername.trim();
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public String getSubSellerid() {
        return subSellerid;
    }

    public void setSubSellerid(String subSellerid) {
        this.subSellerid = subSellerid == null ? null : subSellerid.trim();
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

	public double getTotalLimitRecharge() {
		return totalLimitRecharge;
	}

	public void setTotalLimitRecharge(double totalLimitRecharge) {
		this.totalLimitRecharge = totalLimitRecharge;
	}

	public String getRechargeItem() {
		return rechargeItem;
	}

	public void setRechargeItem(String rechargeItem) {
		this.rechargeItem = rechargeItem;
	}

	public double getReferrerRatio() {
		return referrerRatio;
	}

	public void setReferrerRatio(double referrerRatio) {
		this.referrerRatio = referrerRatio;
	}

	public double getUpparenReferrerRatio() {
		return upparenReferrerRatio;
	}

	public void setUpparenReferrerRatio(double upparenReferrerRatio) {
		this.upparenReferrerRatio = upparenReferrerRatio;
	}

	@Override
	public String toString() {
		return "DebitcardSeller [id=" + id + ", sellerid=" + sellerid
				+ ", sellertype=" + sellertype + ", sellername=" + sellername
				+ ", status=" + status + ", subSellerid=" + subSellerid
				+ ", updateTime=" + updateTime + ", totalLimitRecharge="
				+ totalLimitRecharge + ", rechargeItem=" + rechargeItem
				+ ", referrerRatio=" + referrerRatio
				+ ", upparenReferrerRatio=" + upparenReferrerRatio + "]";
	}
    
}