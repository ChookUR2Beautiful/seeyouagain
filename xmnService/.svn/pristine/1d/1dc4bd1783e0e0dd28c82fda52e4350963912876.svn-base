package com.xmniao.xmn.core.recruit.entity;

import net.sf.oval.constraint.NotNull;

public class EditUserInfo {
	@NotNull(message="用户图像地址不能为空")
	private String photo;//用户图像地址
	@NotNull(message="用户姓名不能为空")
	private String name;//用户姓名
	@NotNull(message="用户年龄不能为空")
	private Integer age;//用户年龄
	@NotNull(message="用户电话不能为空")
	private String phoneid;//用户电话
	@NotNull(message="用户最高学历不能为空")
	private String degrees;//用户最高学历
	@NotNull(message="用户工作经验不能为空")
	private String experie;//薪资要求用户工作经验
	@NotNull(message="薪资要求不能为空")
	private String salary;//薪资要求
	@NotNull(message="工作城市编号不能为空")
	private String workcity;//工作城市编号
	@NotNull(message="用户性别不能为空")
	private String sex;//用户性别
	@NotNull(message="找工作类型的标签不能为空")
	private String jobs;//找工作类型的标签
	@NotNull(message="做过的工作标签不能为空")
	private String works;//做过的工作标签
	@NotNull(message="培训经历标签不能为空")
	private String trains;//培训经历标签
	@NotNull(message="自我评价标签不能为空")
	private String evaluate;//自我评价标签
	
	public String getPhoto() {
		return photo;
	}
	public void setPhoto(String photo) {
		this.photo = photo;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getAge() {
		return age;
	}
	public void setAge(Integer age) {
		this.age = age;
	}
	public String getPhoneid() {
		return phoneid;
	}
	public void setPhoneid(String phoneid) {
		this.phoneid = phoneid;
	}
	public String getDegrees() {
		return degrees;
	}
	public void setDegrees(String degrees) {
		this.degrees = degrees;
	}
	public String getExperie() {
		return experie;
	}
	public void setExperie(String experie) {
		this.experie = experie;
	}
	public String getSalary() {
		return salary;
	}
	public void setSalary(String salary) {
		this.salary = salary;
	}
	public String getWorkcity() {
		return workcity;
	}
	public void setWorkcity(String workcity) {
		this.workcity = workcity;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public String getJobs() {
		return jobs;
	}
	public void setJobs(String jobs) {
		this.jobs = jobs;
	}
	public String getWorks() {
		return works;
	}
	public void setWorks(String works) {
		this.works = works;
	}
	public String getTrains() {
		return trains;
	}
	public void setTrains(String trains) {
		this.trains = trains;
	}
	public String getEvaluate() {
		return evaluate;
	}
	public void setEvaluate(String evaluate) {
		this.evaluate = evaluate;
	}
	@Override
	public String toString() {
		return "EditUserInfo [photo=" + photo + ", name=" + name + ", age="
				+ age + ", phoneid=" + phoneid + ", degrees=" + degrees
				+ ", experie=" + experie + ", salary=" + salary + ", workcity="
				+ workcity + ", sex=" + sex + ", jobs=" + jobs + ", works="
				+ works + ", trains=" + trains + ", evaluate=" + evaluate + "]";
	}
}
