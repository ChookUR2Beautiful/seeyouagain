package com.xmniao.xmn.core.market.entity.cart;

import java.util.List;


public class SaleProperty {
    private Integer id;

    private Long codeid;

    private String property;

    private Integer sort;
    
    private List<SalePropertyValue> subList;
    
    

    public List<SalePropertyValue> getSubList() {
        return subList;
    }

    public void setSubList(List<SalePropertyValue> subList) {
        this.subList = subList;
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Long getCodeid() {
        return codeid;
    }

    public void setCodeid(Long codeid) {
        this.codeid = codeid;
    }

    public String getProperty() {
        return property;
    }

    public void setProperty(String property) {
        this.property = property == null ? null : property.trim();
    }

    public Integer getSort() {
        return sort;
    }

    public void setSort(Integer sort) {
        this.sort = sort;
    }
}