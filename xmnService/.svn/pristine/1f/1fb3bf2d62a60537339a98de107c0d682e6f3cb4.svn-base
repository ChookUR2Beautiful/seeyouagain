package com.xmniao.xmn.core.coupon.response;

import java.io.Serializable;

/**
 * 
* @projectName: xmnService 
* @ClassName: CouponCardsResponse    
* @Description:我的卡卷返回类   
* @author: liuzhihao   
* @date: 2017年2月27日 上午10:25:53
 */
@SuppressWarnings("serial")
public class CouponCardsResponse implements Serializable{
	
	private String id;//优惠卷ID
	
	private String sellerid;//商铺ID
	
	private String denomination;//面额:(充值卡鸟币,套餐价格,优惠卷面额)
	
	private String cname;//名称:(充值卡名称,套餐名称,优惠卷名称)
	
	private String ctype;//卡卷类型 0本地生活类优惠卷(不可使用积分组合支付);1生鲜现金卷(可以和组合支付) 2 粉丝券 3 预售抵用券 
	
	private String useRange;//使用场所(使用范围)
	
	private String validDate;//使用时间(有效期)
	
	private String condition;//使用条件
	
	private String serial;//卡卷二维码地址
	
	private String type;//卡卷使用情况 0 未使用 1过期 2已使用
	
	private String seats;//位置数/可用人数((粉丝卷使用)
	
	private String isOverdue;//是否有效(充值卡鸟币,套餐价格,优惠卷面额) 0 不可以使用 1可以使用
	
	private String sellerType;//店铺类型

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getSellerid() {
		return sellerid;
	}

	public void setSellerid(String sellerid) {
		this.sellerid = sellerid;
	}

	public String getDenomination() {
		return denomination;
	}

	public void setDenomination(String denomination) {
		this.denomination = denomination;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public String getCtype() {
		return ctype;
	}

	public void setCtype(String ctype) {
		this.ctype = ctype;
	}

	public String getUseRange() {
		return useRange;
	}

	public void setUseRange(String useRange) {
		this.useRange = useRange;
	}

	public String getValidDate() {
		return validDate;
	}

	public void setValidDate(String validDate) {
		this.validDate = validDate;
	}

	public String getCondition() {
		return condition;
	}

	public void setCondition(String condition) {
		this.condition = condition;
	}

	public String getSerial() {
		return serial;
	}

	public void setSerial(String serial) {
		this.serial = serial;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getSeats() {
		return seats;
	}

	public void setSeats(String seats) {
		this.seats = seats;
	}

	public String getIsOverdue() {
		return isOverdue;
	}

	public void setIsOverdue(String isOverdue) {
		this.isOverdue = isOverdue;
	}

	public String getSellerType() {
		return sellerType;
	}

	public void setSellerType(String sellerType) {
		this.sellerType = sellerType;
	}
	
}
