<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xmniao.xmn.core.xmer.dao.XmerLevelDao">
	
	<!-- 查询成就详情列表 -->
	<select id="queryXmerLevel" resultType="map">
		SELECT 
			level_no levelno,sign_num signnum,level_name levelname 
		from 
			b_level 
		order by levelno asc
	</select>
	
</mapper>