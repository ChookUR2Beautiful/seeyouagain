package com.xmniao.enums;

/**
 * 黄金装呀红包类型
 *
 * @author liyuanbo
 * @create 2017-08-01 9:56
 **/
public enum ManorPropsRedpackageTypeEnum {

    RANDOM(1, "随机"),
    FIXED(2, "固定"),
    FRIENDS(3, "园又");
    private int type;
    private String name;

    private ManorPropsRedpackageTypeEnum(int type, String name) {
        this.type = type;
        this.name = name;
    }

    public int getType() {
        return type;
    }

    public String getName() {
        return name;
    }
}
