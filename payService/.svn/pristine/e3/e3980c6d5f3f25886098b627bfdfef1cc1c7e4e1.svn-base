public enum ActiveManorConfigTypeEnum {
    ACTIVE_ENGRY_TYPE(1, "能量激活"),
    ACTIVE_SUN_TYPE(2, "阳光激活"),
    RENEW_ENGRY_TYPE(3, "能量续租"),
    RENEW_SUN_TYPE(4, "阳光续租");
    private int type;
    private String remark;

    private ActiveManorConfigTypeEnum(int type, String remark) {
        this.type = type;
        this.remark = remark;
    }

    public int getType() {
        return type;
    }

    public String getRemark() {
        return remark;
    }

    public void setType(int type) {
        this.type = type;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}
