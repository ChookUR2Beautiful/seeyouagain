	 		}else{
	 			return true;
	 		}
		}else{
			return true;
		}
   },"平台补贴必须大于用户占比！");
	
	$.validator.addMethod("accountBy",function(value,element,params){
 		if(value.length > 5){
 			return  false;
 		}
 		if(value >= params[0] && value <= params[1]){
 			return true;
 		}else{
 			return false;
 		}
   },"请输入0-100之间的数");
