  <!--   自定义sql专区 -->
  
   <!-- 庄园会员列表查询条件 -->
  <sql id="baseSellerCondition">
      <if test="id != null" >
          AND t.id = #{id}
      </if>
      <if test="uid != null" >
          AND t.uid = #{uid,jdbcType=INTEGER}
