package com.xmniao.xmn.core.member.entity;

import com.xmniao.xmn.core.base.BaseEntity;

/**
 * 
 * 项目名称：XmnWeb
 * 
 * 类名称：MemberWallet
 * 
 * 类描述： 用户钱包实体类
 * 
 * 创建人：  huang'tao
 * 
 * 创建时间：2017-8-17 下午3:32:22 
 * 
 * Copyright (c) 广东寻蜜鸟网络技术有限公司 
 */

public class MemberWallet extends BaseEntity {

	/**
	 * 
	 */
	private static final long serialVersionUID = -5986344027747600930L;
	
	private Integer uid;//用户UID
	
	private String nname;//用户昵称
	
	private String phone;//手机号码
	
	private String amount;//钱包余额
	
	private String balance;//分账余额
	
	private String commision;//佣金余额
	
	private String zbalance;//赠送余额
	
	private String integral;//积分余额
	
	private String zbalanceLock;//锁定佣金余额标志,true 锁定，false 未锁定
	
	private String updateOption;//操作类型0加锁，1解锁
	
	private String liveBalance;//鸟蛋余额
	
	private String liveCommision;//鸟豆余额
	
	private String liveZbalance;//鸟币余额
	
	private String liveTurnEggOut;//鸟蛋转出总额
	
	private String liveZbalanceLock;//是否锁定鸟币, true 是，false 否

	/**
	 * @return the uid
	 */
	public Integer getUid() {
		return uid;
	}

	/**
	 * @param uid the uid to set
	 */
	public void setUid(Integer uid) {
		this.uid = uid;
	}

	/**
	 * @return the nname
	 */
	public String getNname() {
		return nname;
	}

	/**
	 * @param nname the nname to set
	 */
	public void setNname(String nname) {
		this.nname = nname;
	}

	/**
	 * @return the phone
	 */
	public String getPhone() {
		return phone;
	}

	/**
	 * @param phone the phone to set
	 */
	public void setPhone(String phone) {
		this.phone = phone;
	}

	/**
	 * @return the amount
	 */
	public String getAmount() {
		return amount;
	}

	/**
	 * @param amount the amount to set
	 */
	public void setAmount(String amount) {
		this.amount = amount;
	}

	/**
	 * @return the balance
	 */
	public String getBalance() {
		return balance;
	}

	/**
	 * @param balance the balance to set
	 */
	public void setBalance(String balance) {
		this.balance = balance;
	}

	/**
	 * @return the commision
	 */
	public String getCommision() {
		return commision;
	}

	/**
	 * @param commision the commision to set
	 */
	public void setCommision(String commision) {
		this.commision = commision;
	}

	/**
	 * @return the zbalance
	 */
	public String getZbalance() {
		return zbalance;
	}

	/**
	 * @param zbalance the zbalance to set
	 */
	public void setZbalance(String zbalance) {
		this.zbalance = zbalance;
	}

	/**
	 * @return the integral
	 */
	public String getIntegral() {
		return integral;
	}

	/**
	 * @param integral the integral to set
	 */
	public void setIntegral(String integral) {
		this.integral = integral;
	}

	/**
	 * @return the zbalanceLock
	 */
	public String getZbalanceLock() {
		return zbalanceLock;
	}

	/**
	 * @param zbalanceLock the zbalanceLock to set
	 */
	public void setZbalanceLock(String zbalanceLock) {
		this.zbalanceLock = zbalanceLock;
	}
	
	

	/**
	 * @return the updateOption
	 */
	public String getUpdateOption() {
		return updateOption;
	}

	/**
	 * @param updateOption the updateOption to set
	 */
	public void setUpdateOption(String updateOption) {
		this.updateOption = updateOption;
	}
	
	

	/**
	 * @return the liveBalance
	 */
	public String getLiveBalance() {
		return liveBalance;
	}

	/**
	 * @param liveBalance the liveBalance to set
	 */
	public void setLiveBalance(String liveBalance) {
		this.liveBalance = liveBalance;
	}

	/**
	 * @return the liveCommision
	 */
	public String getLiveCommision() {
		return liveCommision;
	}

	/**
	 * @param liveCommision the liveCommision to set
	 */
	public void setLiveCommision(String liveCommision) {
		this.liveCommision = liveCommision;
	}


	
	/**
	 * @return the liveZbalance
	 */
	public String getLiveZbalance() {
		return liveZbalance;
	}

	/**
	 * @param liveZbalance the liveZbalance to set
	 */
	public void setLiveZbalance(String liveZbalance) {
		this.liveZbalance = liveZbalance;
	}

	/**
	 * @return the liveTurnEggOut
	 */
	public String getLiveTurnEggOut() {
		return liveTurnEggOut;
	}

	/**
	 * @param liveTurnEggOut the liveTurnEggOut to set
	 */
	public void setLiveTurnEggOut(String liveTurnEggOut) {
		this.liveTurnEggOut = liveTurnEggOut;
	}

	/**
	 * @return the liveZbalanceLock
	 */
	public String getLiveZbalanceLock() {
		return liveZbalanceLock;
	}

	/**
	 * @param liveZbalanceLock the liveZbalanceLock to set
	 */
	public void setLiveZbalanceLock(String liveZbalanceLock) {
		this.liveZbalanceLock = liveZbalanceLock;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "MemberWallet [uid=" + uid + ", nname=" + nname + ", phone="
				+ phone + ", amount=" + amount + ", balance=" + balance
				+ ", commision=" + commision + ", zbalance=" + zbalance
				+ ", integral=" + integral + ", zbalanceLock=" + zbalanceLock
				+ "]";
	}
	
	

}
