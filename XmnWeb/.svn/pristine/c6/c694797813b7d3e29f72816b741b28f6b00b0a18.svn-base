package com.xmniao.xmn.core.fresh.entity;

import java.util.Date;

import com.xmniao.xmn.core.base.BaseEntity;

public class FreshWord extends BaseEntity{
    private Long id;

    private Integer type;

    private Long userid;

    private String word;

    private String defaultWord;

    private Date createTime;

    private Date updateTime;
    
    private Long wordSum;
    
    private Integer sort;
    
    private Integer record;
    
    public Integer getRecord() {
		return record;
	}

	public void setRecord(Integer record) {
		this.record = record;
	}

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

	private Integer likeType;	//查询类型  1:名字模糊查询

	public Integer getLikeType() {
		return likeType;
	}

	public void setLikeType(Integer likeType) {
		this.likeType = likeType;
	}

	public Long getWordSum() {
		return wordSum;
	}

	public void setWordSum(Long wordSum) {
		this.wordSum = wordSum;
	}

	public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public Long getUserid() {
        return userid;
    }

    public void setUserid(Long userid) {
        this.userid = userid;
    }

    public String getWord() {
        return word;
    }

    public void setWord(String word) {
        this.word = word == null ? null : word.trim();
    }

    public String getDefaultWord() {
        return defaultWord;
    }

    public void setDefaultWord(String defaultWord) {
        this.defaultWord = defaultWord == null ? null : defaultWord.trim();
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }
}