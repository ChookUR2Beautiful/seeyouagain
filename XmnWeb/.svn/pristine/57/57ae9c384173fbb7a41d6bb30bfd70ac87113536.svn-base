package com.xmniao.xmn.core.manor.entity;

import java.util.Date;

public class TManorFlower {
    private Integer id;

    private String branchId;

    private Integer type;

    private Integer floristUid;

    private Integer uid;

    private Date perishTime;
    
    /*自定义字段区域*/
    private Integer currentFlowerRelationCount;  //当前贡献花朵
    
    private Integer totalFlowerRelationCount;  //累计贡献花朵
    
    private String floristName;
    

    public Integer getCurrentFlowerRelationCount() {
		return currentFlowerRelationCount;
	}

	public void setCurrentFlowerRelationCount(Integer currentFlowerRelationCount) {
		this.currentFlowerRelationCount = currentFlowerRelationCount;
	}

	public Integer getTotalFlowerRelationCount() {
		return totalFlowerRelationCount;
	}

	public void setTotalFlowerRelationCount(Integer totalFlowerRelationCount) {
		this.totalFlowerRelationCount = totalFlowerRelationCount;
	}
	
	public String getFloristName() {
		return floristName;
	}

	public void setFloristName(String floristName) {
		this.floristName = floristName;
	}
	
	 /*自定义字段区域*/



	public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getBranchId() {
        return branchId;
    }

    public void setBranchId(String branchId) {
        this.branchId = branchId == null ? null : branchId.trim();
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public Integer getFloristUid() {
        return floristUid;
    }

    public void setFloristUid(Integer floristUid) {
        this.floristUid = floristUid;
    }

    public Integer getUid() {
        return uid;
    }

    public void setUid(Integer uid) {
        this.uid = uid;
    }

    public Date getPerishTime() {
        return perishTime;
    }

    public void setPerishTime(Date perishTime) {
        this.perishTime = perishTime;
    }
}