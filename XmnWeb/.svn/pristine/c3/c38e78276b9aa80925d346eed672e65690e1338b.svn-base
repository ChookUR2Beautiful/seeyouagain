package com.xmniao.xmn.core.businessman.entity;

import java.math.BigDecimal;
import java.util.Date;

import com.xmniao.xmn.core.base.BaseEntity;

/**
 * 
 * 
 * 项目名称：XmnWeb
 * 
 * 类名称：VipCardBean
 * 
 * 类描述： 商户会员卡管理之列表数据
 * 
 * 创建人： Chen Bo
 * 
 * 创建时间：2016年1月20日 下午6:33:06 
 * 
 * Copyright (c) 广东寻蜜鸟网络技术有限公司
 */
public class VipCardBean extends BaseEntity{
	
 	private static final long serialVersionUID = 1L;

 	/*
 	 * 主键ID,配置表ID
 	 */
	private Integer id;

	/*
	 * 总店商家编号
	 */
    private Integer sellerId;

    /*
     * 总店商家名称
     */
    private String sellerName;

    /*
     * 会员卡LOGO路径
     */
    private String sellerLogo;

    /*
     * 会员卡名称
     */
    private String cardName;

    /*
     * 是否开启支付功能
     */
    private Integer isPay;

    /*
     * 会员卡用户数量
     */
    private Integer activeNum;

    /*
     * 适用商家数
     */
    private Integer sellerNum;

    /*
     * 会员特权
     */
    private String rights;

    /*
     * 该商户会员卡充值总额
     */
    private BigDecimal amount;

    /*
     * 会员卡状态
     */
    private Integer cardStatus;

    /*
     * 创建时间
     */
    private Date rdate;

    /*
     * 更新时间
     */
    private Date udate;

    /*
     * 数据状态
     */
    private Integer dstatus;
    
    /*
     * 充值方案 如：充100.00,到账150.00;充200.00,到500.00
     */
    private String prepaidScheme;
    
    /*
     * 该商户会员卡已使用总金额
     */
    private BigDecimal usedAmount;
    
    /*
     * 该商户会员卡会员卡总余额
     */
    private BigDecimal notUsedAmount;

    /*
     * 最少会员卡数
     */
    private Integer minActiveNum;
    
    /*
     * 最多会员卡数
     */
    private Integer maxActiveNum;
    
    /*
     * 最少总余额
     */
    private BigDecimal minNotUsedAmount;
    
    /*
     * 最多总余额
     */
    private BigDecimal maxNotUsedAmount;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getSellerId() {
		return sellerId;
	}

	public void setSellerId(Integer sellerId) {
		this.sellerId = sellerId;
	}

	public String getSellerName() {
		return sellerName;
	}

	public void setSellerName(String sellerName) {
		this.sellerName = sellerName;
	}


	public String getSellerLogo() {
		return sellerLogo;
	}

	public void setSellerLogo(String sellerLogo) {
		this.sellerLogo = sellerLogo;
	}

	public String getCardName() {
		return cardName;
	}

	public void setCardName(String cardName) {
		this.cardName = cardName;
	}

	public Integer getIsPay() {
		return isPay;
	}

	public void setIsPay(Integer isPay) {
		this.isPay = isPay;
	}

	public Integer getActiveNum() {
		return activeNum;
	}

	public void setActiveNum(Integer activeNum) {
		this.activeNum = activeNum;
	}

	public Integer getSellerNum() {
		return sellerNum;
	}

	public void setSellerNum(Integer sellerNum) {
		this.sellerNum = sellerNum;
	}

	public String getRights() {
		return rights;
	}

	public void setRights(String rights) {
		this.rights = rights;
	}

	public BigDecimal getAmount() {
		return amount;
	}

	public void setAmount(BigDecimal amount) {
		this.amount = amount;
	}

	public Integer getCardStatus() {
		return cardStatus;
	}

	public void setCardStatus(Integer cardStatus) {
		this.cardStatus = cardStatus;
	}

	public Date getRdate() {
		return rdate;
	}

	public void setRdate(Date rdate) {
		this.rdate = rdate;
	}

	public Date getUdate() {
		return udate;
	}

	public void setUdate(Date udate) {
		this.udate = udate;
	}

	public Integer getDstatus() {
		return dstatus;
	}

	public void setDstatus(Integer dstatus) {
		this.dstatus = dstatus;
	}

	public String getPrepaidScheme() {
		return prepaidScheme;
	}

	public void setPrepaidScheme(String prepaidScheme) {
		this.prepaidScheme = prepaidScheme;
	}

	public BigDecimal getUsedAmount() {
		return usedAmount;
	}

	public void setUsedAmount(BigDecimal usedAmount) {
		this.usedAmount = usedAmount;
	}

	public BigDecimal getNotUsedAmount() {
		return notUsedAmount;
	}

	public void setNotUsedAmount(BigDecimal notUsedAmount) {
		this.notUsedAmount = notUsedAmount;
	}

	public Integer getMinActiveNum() {
		return minActiveNum;
	}

	public void setMinActiveNum(Integer minActiveNum) {
		this.minActiveNum = minActiveNum;
	}

	public Integer getMaxActiveNum() {
		return maxActiveNum;
	}

	public void setMaxActiveNum(Integer maxActiveNum) {
		this.maxActiveNum = maxActiveNum;
	}

	public BigDecimal getMinNotUsedAmount() {
		return minNotUsedAmount;
	}

	public void setMinNotUsedAmount(BigDecimal minNotUsedAmount) {
		this.minNotUsedAmount = minNotUsedAmount;
	}

	public BigDecimal getMaxNotUsedAmount() {
		return maxNotUsedAmount;
	}

	public void setMaxNotUsedAmount(BigDecimal maxNotUsedAmount) {
		this.maxNotUsedAmount = maxNotUsedAmount;
	}

	@Override
	public String toString() {
		return "VipCardBean [id=" + id + ", sellerId=" + sellerId
				+ ", sellerName=" + sellerName + ", sellerLogo=" + sellerLogo
				+ ", cardName=" + cardName + ", isPay=" + isPay
				+ ", activeNum=" + activeNum + ", sellerNum=" + sellerNum
				+ ", rights=" + rights + ", amount=" + amount + ", cardStatus="
				+ cardStatus + ", rdate=" + rdate + ", udate=" + udate
				+ ", dstatus=" + dstatus + ", prepaidScheme=" + prepaidScheme
				+ ", usedAmount=" + usedAmount + ", notUsedAmount="
				+ notUsedAmount + ", minActiveNum=" + minActiveNum
				+ ", maxActiveNum=" + maxActiveNum + ", minNotUsedAmount="
				+ minNotUsedAmount + ", maxNotUsedAmount=" + maxNotUsedAmount
				+ "]";
	}
}
