package com.xmniao.xmn.core.fresh.entity;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;

import com.xmniao.xmn.core.base.BaseEntity;

public class ActivityProduct extends BaseEntity{
    private Integer id;

    private Integer activityId;

    private Integer productId;

    private Integer groupId;

    private Long codeId;

    private Integer activityType;

    private Integer beforeStore;

    private Integer sellStore;

    private BigDecimal sellIntegral;

    private BigDecimal basePrice;

    private BigDecimal salePrice;

    private Integer sort;

    private Integer type;

    private Integer status;

    private Date createTime;

    private Date updateTime;
    
    private Long moduleId;
    
    private List<ActivityGroup>  activityGroups;
    
    private String pic;	//图片
    
    private String pname; //商品名称
    
    private String productJsonString;
    
    public String getProductJsonString() {
		return productJsonString;
	}

	public void setProductJsonString(String productJsonString) {
		this.productJsonString = productJsonString;
	}

	public Long getModuleId() {
		return moduleId;
	}

	public void setModuleId(Long moduleId) {
		this.moduleId = moduleId;
	}

	public String getPname() {
		return pname;
	}

	public void setPname(String pname) {
		this.pname = pname;
	}

	public String getPic() {
		return pic;
	}

	public void setPic(String pic) {
		this.pic = pic;
	}

	public Long getCodeId() {
		return codeId;
	}

	public void setCodeId(Long codeId) {
		this.codeId = codeId;
	}

	public List<ActivityGroup> getActivityGroups() {
		return activityGroups;
	}

	public void setActivityGroups(List<ActivityGroup> activityGroups) {
		this.activityGroups = activityGroups;
	}

	public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getActivityId() {
        return activityId;
    }

    public void setActivityId(Integer activityId) {
        this.activityId = activityId;
    }

    public Integer getProductId() {
        return productId;
    }

    public void setProductId(Integer productId) {
        this.productId = productId;
    }

    public Integer getGroupId() {
        return groupId;
    }

    public void setGroupId(Integer groupId) {
        this.groupId = groupId;
    }


    public Integer getActivityType() {
        return activityType;
    }

    public void setActivityType(Integer activityType) {
        this.activityType = activityType;
    }

    public Integer getBeforeStore() {
        return beforeStore;
    }

    public void setBeforeStore(Integer beforeStore) {
        this.beforeStore = beforeStore;
    }

    public Integer getSellStore() {
        return sellStore;
    }

    public void setSellStore(Integer sellStore) {
        this.sellStore = sellStore;
    }

    public BigDecimal getSellIntegral() {
        return sellIntegral;
    }

    public void setSellIntegral(BigDecimal sellIntegral) {
        this.sellIntegral = sellIntegral;
    }

    public BigDecimal getBasePrice() {
        return basePrice;
    }

    public void setBasePrice(BigDecimal basePrice) {
        this.basePrice = basePrice;
    }

    public BigDecimal getSalePrice() {
        return salePrice;
    }

    public void setSalePrice(BigDecimal salePrice) {
        this.salePrice = salePrice;
    }

    public Integer getSort() {
        return sort;
    }

    public void setSort(Integer sort) {
        this.sort = sort;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

	@Override
	public String toString() {
		return "ActivityProduct [id=" + id + ", activityId=" + activityId + ", productId=" + productId + ", groupId="
				+ groupId + ", codeid=" + codeId + ", activityType=" + activityType + ", beforeStore=" + beforeStore
				+ ", sellStore=" + sellStore + ", sellIntegral=" + sellIntegral + ", basePrice=" + basePrice
				+ ", salePrice=" + salePrice + ", sort=" + sort + ", type=" + type + ", status=" + status
				+ ", createTime=" + createTime + ", updateTime=" + updateTime + "]";
	}
    
    
}