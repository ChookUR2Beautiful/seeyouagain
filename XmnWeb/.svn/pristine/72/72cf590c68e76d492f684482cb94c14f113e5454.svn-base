		private int index;

		private SendStatus(String name, Integer index) {
			this.index = index;
			this.name = name;
		}

		public static String getName(int index) {
			for (SendStatus c : SendStatus.values()) {
				if (c.getIndex() == index) {
					return c.name;
				}
			}
			return null;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}

		public int getIndex() {
			return index;
		}

		public void setIndex(int index) {
			this.index = index;
		}
	}

