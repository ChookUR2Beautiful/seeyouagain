	public long getRankDetailNum() {
		return rankDetailNum;
	}

	/**
	 * @param rankDetailNum the rankDetailNum to set
	 */
	public void setRankDetailNum(long rankDetailNum) {
		this.rankDetailNum = rankDetailNum;
	}
	
	

	/**
