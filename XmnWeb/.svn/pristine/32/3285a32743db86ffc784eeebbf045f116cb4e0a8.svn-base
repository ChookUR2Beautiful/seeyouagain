package com.xmniao.xmn.core.common.entity;

import java.util.Date;
import java.util.List;

import com.xmniao.xmn.core.base.BaseEntity;

/**
 * 项目名称：XmnWeb
 * 
 * 类名称：TArea
 * 
 * 类描述：区域
 * 
 * 创建人： zhou'sheng
 * 
 * 创建时间：2014年11月12日17时37分19秒
 * 
 * Copyright (c) 广东寻蜜鸟网络技术有限公司
 */
public class TArea extends BaseEntity {

	private static final long serialVersionUID = -295247304772485916L;
	private Integer areaId;// 区域ID
	private String title;// 区域名称
	private Integer pid;// 区域父ID
	private String regionId;// 区号
	private Integer status;// 默认0开启
	private String firstletter;// 区域首字母
	private Integer hot;// 是否为热门
	private Date sdate;// 创建时间
	private Byte isDel;// 是否删除
	private String shorts;// 字母简写
	private String pinyin;// 拼音
	private Integer upid;// 区域首字母
	private String locationId;// 地区编号
	private Integer isCity;// 是否为城市
	
	private String corporate;// 合作商名称
	private Integer bnum;// 商圈数
	private String ptitle;// 省
	private String ctitle;// 市
	private String atitle;// 区
	private Integer pareaId;// 省编号
	private Integer careaId;// 区编号
	private Integer aareaId;// 市编号
	
	private Integer openNum;  //城市已开通数
	private Integer lockNum;  //城市未开通数
	
	private String btitle;
	
	private String bid;  //商圈主键ID
	
	private List<City> cityList;//城市列表
	
	public List<City> getCityList() {
		return cityList;
	}

	public void setCityList(List<City> cityList) {
		this.cityList = cityList;
	}

	/**
	 * 创建一个新的实例 TArea.
	 * 
	 */
	public TArea() {
		super();
	}

	public String getBid() {
		return bid;
	}

	public void setBid(String bid) {
		this.bid = bid;
	}

	public Integer getOpenNum() {
		return openNum;
	}

	public void setOpenNum(Integer openNum) {
		this.openNum = openNum;
	}

	public Integer getLockNum() {
		return lockNum;
	}

	public void setLockNum(Integer lockNum) {
		this.lockNum = lockNum;
	}

	public TArea(Integer areaId) {
		this.areaId = areaId;
	}

	/**
	 * areaId
	 * 
	 * @return the areaId
	 */
	public Integer getAreaId() {
		return areaId;
	}

	/**
	 * @param areaId
	 *            the areaId to set
	 */
	public void setAreaId(Integer areaId) {
		this.areaId = areaId;
	}

	/**
	 * title
	 * 
	 * @return the title
	 */
	public String getTitle() {
		return title;
	}

	/**
	 * @param title
	 *            the title to set
	 */
	public void setTitle(String title) {
		this.title = title;
	}

	public String getBtitle() {
		return btitle;
	}

	public void setBtitle(String btitle) {
		this.btitle = btitle;
	}

	/**
	 * pid
	 * 
	 * @return the pid
	 */
	public Integer getPid() {
		return pid;
	}

	/**
	 * @param pid
	 *            the pid to set
	 */
	public void setPid(Integer pid) {
		this.pid = pid;
	}

	/**
	 * regionId
	 * 
	 * @return the regionId
	 */
	public String getRegionId() {
		return regionId;
	}

	/**
	 * @param regionId
	 *            the regionId to set
	 */
	public void setRegionId(String regionId) {
		this.regionId = regionId;
	}

	/**
	 * status
	 * 
	 * @return the status
	 */
	public Integer getStatus() {
		return status;
	}

	/**
	 * @param status
	 *            the status to set
	 */
	public void setStatus(Integer status) {
		this.status = status;
	}

	/**
	 * firstletter
	 * 
	 * @return the firstletter
	 */
	public String getFirstletter() {
		return firstletter;
	}

	/**
	 * @param firstletter
	 *            the firstletter to set
	 */
	public void setFirstletter(String firstletter) {
		this.firstletter = firstletter;
	}

	public Integer getHot() {
		return hot;
	}

	public void setHot(Integer hot) {
		this.hot = hot;
	}

	public Date getSdate() {
		return sdate;
	}

	public void setSdate(Date sdate) {
		this.sdate = sdate;
	}

	public Byte getIsDel() {
		return isDel;
	}

	public void setIsDel(Byte isDel) {
		this.isDel = isDel;
	}

	public String getShorts() {
		return shorts;
	}

	public void setShorts(String shorts) {
		this.shorts = shorts;
	}

	public String getPinyin() {
		return pinyin;
	}

	public void setPinyin(String pinyin) {
		this.pinyin = pinyin;
	}

	public Integer getUpid() {
		return upid;
	}

	public void setUpid(Integer upid) {
		this.upid = upid;
	}

	public String getLocationId() {
		return locationId;
	}

	public void setLocationId(String locationId) {
		this.locationId = locationId;
	}

	public Integer getIsCity() {
		return isCity;
	}

	public void setIsCity(Integer isCity) {
		this.isCity = isCity;
	}
	
	
	/**
	 * @return the corporate
	 */
	public String getCorporate() {
		return corporate;
	}

	/**
	 * @param corporate the corporate to set
	 */
	public void setCorporate(String corporate) {
		this.corporate = corporate;
	}

	/**
	 * @return the bnum
	 */
	public Integer getBnum() {
		return bnum;
	}

	/**
	 * @param bnum the bnum to set
	 */
	public void setBnum(Integer bnum) {
		this.bnum = bnum;
	}

	/**
	 * @return the ptitle
	 */
	public String getPtitle() {
		return ptitle;
	}

	/**
	 * @param ptitle the ptitle to set
	 */
	public void setPtitle(String ptitle) {
		this.ptitle = ptitle;
	}

	/**
	 * @return the ctitle
	 */
	public String getCtitle() {
		return ctitle;
	}

	/**
	 * @param ctitle the ctitle to set
	 */
	public void setCtitle(String ctitle) {
		this.ctitle = ctitle;
	}

	/**
	 * @return the atitle
	 */
	public String getAtitle() {
		return atitle;
	}

	/**
	 * @param atitle the atitle to set
	 */
	public void setAtitle(String atitle) {
		this.atitle = atitle;
	}

	/**
	 * @return the pareaId
	 */
	public Integer getPareaId() {
		return pareaId;
	}

	/**
	 * @param pareaId the pareaId to set
	 */
	public void setPareaId(Integer pareaId) {
		this.pareaId = pareaId;
	}

	/**
	 * @return the careaId
	 */
	public Integer getCareaId() {
		return careaId;
	}

	/**
	 * @param careaId the careaId to set
	 */
	public void setCareaId(Integer careaId) {
		this.careaId = careaId;
	}

	/**
	 * @return the aareaId
	 */
	public Integer getAareaId() {
		return aareaId;
	}

	/**
	 * @param aareaId the aareaId to set
	 */
	public void setAareaId(Integer aareaId) {
		this.aareaId = aareaId;
	}

	@Override
	public String toString() {
		return "TArea [areaId=" + areaId + ", title=" + title + ", pid=" + pid
				+ ", regionId=" + regionId + ", status=" + status
				+ ", firstletter=" + firstletter + ", hot=" + hot + ", sdate="
				+ sdate + ", isDel=" + isDel + ", shorts=" + shorts
				+ ", pinyin=" + pinyin + ", upid=" + upid + ", locationId="
				+ locationId + ", isCity=" + isCity + ", corporate="
				+ corporate + ", bnum=" + bnum + ", ptitle=" + ptitle
				+ ", ctitle=" + ctitle + ", atitle=" + atitle + ", pareaId="
				+ pareaId + ", careaId=" + careaId + ", aareaId=" + aareaId
				+ ", openNum=" + openNum + ", lockNum=" + lockNum + ", btitle="
				+ btitle + ", bid=" + bid + ", cityList=" + cityList + "]";
	}
}
